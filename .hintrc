{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "aspect-ratio",
          "height: fit-content",
          "width: fit-content"
        ]
      }
    ],
    "highest-available-document-mode": "off",
    "button-type": "off",
    "axe/name-role-value": "off",
    "no-inline-styles": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "detect-css-reflows/composite": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 14.8",
    "not and_ff <= 110",
    "not firefox <= 111"
  ]
}